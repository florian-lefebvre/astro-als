{
	"name": "astro-als",
	"version": "0.0.3",
	"description": "Avoid hydration mismatches by getting the data on both server and client using an AsyncLocalStorage in Astro.",
	"author": {
		"email": "contact@florian-lefebvre.dev",
		"name": "Florian Lefebvre",
		"url": "https://florian-lefebvre.dev"
	},
	"license": "MIT",
	"keywords": [
		"astro-integration",
		"astro-component",
		"withastro",
		"astro",
		"performance",
		"seo",
		"ui"
	],
	"homepage": "https://github.com/florian-lefebvre/astro-als",
	"publishConfig": {
		"access": "public"
	},
	"sideEffects": false,
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./config": {
			"types": "./dist/config.d.ts",
			"default": "./dist/config.js"
		},
		"./AlsSerialize.astro": "./assets/AlsSerialize.astro"
	},
	"files": [
		"dist",
		"assets"
	],
	"scripts": {
		"dev": "tsup --watch",
		"build": "tsup"
	},
	"type": "module",
	"peerDependencies": {
		"astro": "^4.11.3"
	},
	"dependencies": {
		"astro-integration-kit": "^0.14.0",
		"vite": "^5.3.2"
	},
	"devDependencies": {
		"tsup": "^8.1.0"
	}
}
